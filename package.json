{
  "name": "bytez-chrome-extension",
  "version": "1.0.3",
  "description": "Bytez chrome extension",
  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "mdi-react": "^7.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "npm-watch": "^0.7.0",
    "prettier": "^2.1.2",
    "react-scripts": "^3.4.3",
    "typescript": "^3.8.3",
    "zip-a-folder": "^0.0.12"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch": "npm-watch",
    "semantic": "node updateVersion.js -",
    "patch": "npm run build && npm run semantic patch",
    "minor": "npm run build && npm run semantic minor",
    "major": "npm run build && npm run semantic major"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "sideEffects": false,
  "watch": {
    "build": {
      "patterns": [
        "public",
        "src"
      ],
      "extensions": "js,jsx"
    }
  }
}